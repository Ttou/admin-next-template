  "license": "MulanPSL2",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "analyzer": "vite build --mode=analyzer",
    "serve": "vite preview --mode=production",
    "scripts": "tsx scripts",
    "lint:js": "eslint \"**/*.{ts,tsx,vue}\" --fix",
    "lint:css": "stylelint \"**/*.{css,vue}\" --fix",
    "check": "vue-tsc -p tsconfig.app.json --noEmit",
    "commit": "git add . && git cz",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@iconify-json/ep": "1.1.15",
    "@iconify-json/icon-park-outline": "1.1.15",
    "@iconify/vue": "4.1.2",
    "@vueuse/core": "10.9.0",
    "@vueuse/integrations": "10.9.0",
    "@wangeditor/editor": "5.1.23",
    "@wangeditor/editor-for-vue": "5.1.12",
    "axios": "1.7.2",
    "axios-mock-adapter": "1.22.0",
    "case-anything": "2.1.13",
    "dayjs": "1.11.11",
    "echarts": "5.5.0",
    "element-plus": "2.7.3",
    "lodash-unified": "1.0.3",
    "nanoid": "5.0.7",
    "nprogress": "0.2.0",
    "pinia": "2.1.7",
    "qrcode": "1.5.3",
    "query-string": "9.0.0",
    "vue": "3.4.27",
    "vue-echarts": "6.7.2",
    "vue-router": "4.3.2",
    "vue-types": "5.1.2",
    "vxe-table": "4.6.16",
    "vxe-table-plugin-element": "4.0.2",
    "xe-utils": "3.5.26"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@iconify/tools": "^4.0.4",
    "@iconify/types": "^2.0.0",
    "@inquirer/checkbox": "^2.3.4",
    "@tsconfig/node18": "^18.2.4",
    "@ttou/vite-html": "^0.0.5",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^18.19.33",
    "@types/nprogress": "^0.2.3",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-legacy": "^5.4.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/runtime-core": "^3.4.27",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "chalk": "^5.3.0",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "figures": "^6.1.0",
    "globby": "^14.0.1",
    "lint-staged": "^15.2.4",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.38",
    "postcss-html": "^1.7.0",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.2.5",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^16.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-prettier": "^5.0.0",
    "tsx": "^4.11.0",
    "type-fest": "^4.18.2",
    "typescript": "^5.4.5",
    "unplugin-imagemin": "^0.5.18",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.2.11",
    "vite-bundle-analyzer": "^0.9.4",
    "vite-plugin-compression2": "^1.1.0",
    "vue-demi": "^0.14.7",
    "vue-tsc": "^2.0.19"
  },
  "browserslist": [
    "defaults",
    "not ie 11"
  ],
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,vue}": [
      "stylelint --fix --allow-empty-input",
      "prettier --write"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx commitlint --edit $1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "@wangeditor/editor-for-vue@5.1.12": "patches/@wangeditor__editor-for-vue@5.1.12.patch"
    }
  }
}
