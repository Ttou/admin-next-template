{
  "name": "admin-next-template",
  "type": "module",
  "author": "WangRuiJin <1361572192@qq.com>",
  "repository": "git@gitee.com:jh_shot/admin-next-template.git",
  "license": "MulanPSL2",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview --mode=production",
    "gen:icons": "tsx scripts/genIcons.ts",
    "lint": "concurrently \"npm:lint:*\"",
    "lint:script": "eslint \"./src/**/*.{ts,tsx,vue}\" --fix",
    "lint:style": "stylelint \"./src/**/*.{css,vue}\" --fix",
    "check": "vue-tsc --noEmit",
    "commit": "git add . && git cz",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@iconify-json/ep": "1.1.14",
    "@iconify-json/icon-park-outline": "1.1.15",
    "@iconify/vue": "4.1.1",
    "@vueuse/core": "10.8.0",
    "@vueuse/integrations": "10.8.0",
    "axios": "1.6.7",
    "axios-mock-adapter": "1.22.0",
    "case-anything": "2.1.13",
    "dayjs": "1.11.10",
    "echarts": "5.5.0",
    "element-plus": "2.5.6",
    "lodash-unified": "1.0.3",
    "nanoid": "5.0.6",
    "nprogress": "0.2.0",
    "pinia": "2.1.7",
    "qrcode": "1.5.3",
    "query-string": "8.2.0",
    "vue": "3.4.19",
    "vue-echarts": "6.6.9",
    "vue-router": "4.2.5",
    "vue-types": "5.1.1",
    "vxe-table": "4.5.18",
    "vxe-table-plugin-element": "4.0.0",
    "xe-utils": "3.5.20"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@iconify/tools": "^4.0.2",
    "@iconify/types": "^2.0.0",
    "@ttou/define-config": "^3.2.0",
    "@ttou/ueditor-typings": "^1.0.0",
    "@ttou/vite-html": "^0.0.5",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^18.19.17",
    "@types/nprogress": "^0.2.3",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-legacy": "^5.3.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.17",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.2",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-vue": "^9.21.1",
    "globby": "^14.0.1",
    "lint-staged": "^15.2.2",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.35",
    "postcss-html": "^1.6.0",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.2.5",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-prettier": "^5.0.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "unplugin-imagemin": "^0.5.18",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.3",
    "vite-plugin-eslint2": "^4.3.1",
    "vite-plugin-stylelint": "^5.3.1",
    "vue-demi": "^0.14.7",
    "vue-tsc": "^1.8.27"
  },
  "browserslist": [
    "defaults",
    "not ie 11"
  ],
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,vue}": [
      "stylelint --fix --allow-empty-input",
      "prettier --write"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx commitlint --edit $1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
