{
  "name": "admin-next-template",
  "type": "module",
  "author": "WangRuiJin <1361572192@qq.com>",
  "repository": "git@gitee.com:jh_shot/admin-next-template.git",
  "license": "MulanPSL2",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "analyzer": "vite build --mode=analyzer",
    "serve": "vite preview --mode=production",
    "scripts": "tsx scripts",
    "lint:js": "eslint \"**/*.{ts,tsx,vue}\" --fix",
    "lint:css": "stylelint \"**/*.{css,vue}\" --fix",
    "check": "vue-tsc -p tsconfig.app.json --noEmit",
    "commit": "git add . && git cz",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@iconify-json/ep": "1.2.0",
    "@iconify-json/icon-park-outline": "1.2.0",
    "@iconify/vue": "4.1.2",
    "@vueuse/core": "11.1.0",
    "@vueuse/integrations": "11.1.0",
    "aieditor": "1.1.2",
    "axios": "1.7.7",
    "axios-mock-adapter": "2.0.0",
    "case-anything": "3.1.0",
    "dayjs": "1.11.13",
    "echarts": "5.5.1",
    "element-plus": "2.8.3",
    "lodash-unified": "1.0.3",
    "nanoid": "5.0.7",
    "nprogress": "0.2.0",
    "pinia": "2.2.2",
    "qrcode": "1.5.4",
    "query-string": "9.1.0",
    "vue": "3.5.7",
    "vue-echarts": "7.0.3",
    "vue-i18n": "^10.0.1",
    "vue-router": "4.4.5",
    "vue-types": "5.1.3",
    "vxe-table": "4.6.19",
    "vxe-table-plugin-element": "4.0.4",
    "xe-utils": "3.5.30"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@iconify/tools": "^4.0.6",
    "@iconify/types": "^2.0.0",
    "@inquirer/checkbox": "^2.5.0",
    "@ttou/vite-html": "^0.0.6",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^18.19.34",
    "@types/nprogress": "^0.2.3",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-legacy": "^5.4.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-typescript": "^13.0.0",
    "autoprefixer": "^10.4.20",
    "chalk": "^5.3.0",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.2.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^9.28.0",
    "figures": "^6.1.0",
    "globby": "^14.0.2",
    "lint-staged": "^15.2.10",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.47",
    "postcss-html": "^1.7.0",
    "postcss-nested": "^6.2.0",
    "prettier": "^3.3.3",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^16.9.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-prettier": "^5.0.2",
    "tsx": "^4.19.1",
    "type-fest": "^4.26.1",
    "typescript": "^5.6.2",
    "unplugin-imagemin": "^0.5.20",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.7",
    "vite-bundle-analyzer": "^0.11.1",
    "vite-plugin-compression2": "^1.3.0",
    "vite-plugin-vue-devtools": "^7.4.5",
    "vue-demi": "^0.14.10",
    "vue-tsc": "^2.1.6"
  },
  "browserslist": [
    "defaults",
    "not ie 11"
  ],
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,vue}": [
      "stylelint --fix --allow-empty-input",
      "prettier --write"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx commitlint --edit $1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
